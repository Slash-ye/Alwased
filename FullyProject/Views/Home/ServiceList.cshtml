@model IEnumerable<FullyProject.Models.Service>
@{
 Layout = null;
}

<h3 class="text-center">خدمات الوكالة</h3>
<br>
@foreach (var s in Model)
{
    <div class="card text-center">
        <img class="card-img-top" src=@s.image1 alt="Card image cap">
        <div class="card-body">
            <h3 class="card-title">@s.name</h3>
            <p class="card-text">@s.description</p>

            <hr>
        </div>
    </div><br>
}


